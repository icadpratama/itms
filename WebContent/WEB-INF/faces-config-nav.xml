<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    <!-- From Home -->
    <navigation-rule>
  		<from-view-id>/home.xhtml</from-view-id>
	  	<navigation-case>
	  		<from-outcome>goToHome</from-outcome>
	   		<to-view-id>/home.xhtml</to-view-id>
	   		<redirect/>
	  	</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
  		<from-view-id>/home.xhtml</from-view-id>
	  	<navigation-case>
	  		<from-outcome>goToLoginLogout</from-outcome>
	   		<to-view-id>/loginlogout.xhtml</to-view-id>
	   		<redirect/>
	  	</navigation-case>
	</navigation-rule>
    
    
    <navigation-rule>
  		<from-view-id>/loginlogout.xhtml</from-view-id>
	  	<navigation-case>
	  		<from-outcome>goToHome</from-outcome>
	   		<to-view-id>/home.xhtml</to-view-id>
	   		<redirect/>
	  	</navigation-case>
	</navigation-rule>
	
	<!-- From Error Page -->
	<navigation-rule>
  		<from-view-id>/error.xhtml</from-view-id>
	  	<navigation-case>
	  		<from-outcome>goToHome</from-outcome>
	   		<to-view-id>/home.xhtml</to-view-id>
	   		<redirect/>
	  	</navigation-case>
	</navigation-rule>
</faces-config>